<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bgm"
    tools:context=".NewPass">

    <View
        android:id="@+id/backtomain"
        android:layout_width="50dp"
        android:layout_height="40dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/ic_baseline_arrow_back_ios_24"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textviewnew"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:fontFamily="@font/manropebold"
        android:text="Add\nYour\nPassword"
        android:textColor="@color/white"
        android:textSize="30dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/backtomain"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/viewnewbg"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginHorizontal="30dp"
        android:layout_marginTop="160dp"
        android:layout_marginBottom="30dp"
        android:background="@drawable/mandombg"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textviewnew"
        app:layout_constraintVertical_bias="0.0" />

    <View
        android:id="@+id/view6"
        android:layout_width="match_parent"
        android:layout_height="130dp"
        android:layout_marginHorizontal="30dp"
        android:layout_marginTop="50dp"
        android:background="@drawable/newpgdesign"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textviewnew" />

    <View
        android:id="@+id/view7"
        android:layout_width="match_parent"
        android:layout_height="130dp"
        android:layout_marginHorizontal="30dp"
        android:layout_marginTop="200dp"
        android:background="@drawable/newpgdesign"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.733"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textviewnew" />

    <View
        android:id="@+id/view8"
        android:layout_width="match_parent"
        android:layout_height="130dp"
        android:layout_marginHorizontal="30dp"
        android:layout_marginTop="350dp"
        android:background="@drawable/newpgdesign"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.733"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textviewnew" />

    <TextView
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="10dp"
        android:elevation="6dp"
        android:fontFamily="@font/manropebold"
        android:gravity="center"
        android:text="Service"
        android:textColor="@color/lightblue"
        android:textSize="25dp"
        app:layout_constraintStart_toStartOf="@+id/view6"
        app:layout_constraintTop_toTopOf="@+id/view6" />

    <TextView
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="10dp"
        android:elevation="6dp"
        android:fontFamily="@font/manropebold"
        android:gravity="center"
        android:text="Password"
        android:textColor="@color/green"
        android:textSize="25dp"
        app:layout_constraintStart_toStartOf="@+id/view8"
        app:layout_constraintTop_toTopOf="@+id/view8" />

    <TextView
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="10dp"
        android:elevation="6dp"
        android:fontFamily="@font/manropebold"
        android:gravity="center"
        android:text="Login"
        android:textColor="@color/pink"
        android:textSize="25dp"
        app:layout_constraintStart_toStartOf="@+id/viewnewbg"
        app:layout_constraintTop_toTopOf="@+id/view7" />

    <AutoCompleteTextView
        android:id="@+id/serviceAutoComplete"
        android:layout_width="250dp"
        android:layout_height="45dp"
        android:layout_marginTop="70dp"
        android:elevation="7dp"
        android:gravity="center_vertical"
        android:paddingStart="30dp"
        android:textSize="15dp"
        android:textColor="@color/black"
        android:background="@drawable/btnbg"
        android:fontFamily="@font/robotoregular"
        app:layout_constraintEnd_toEndOf="@+id/view6"
        app:layout_constraintStart_toStartOf="@+id/view6"
        app:layout_constraintTop_toTopOf="@+id/view6"
        android:hint="Service"
        android:inputType="text"
        android:textColorHint="@android:color/darker_gray"
        android:completionThreshold="1" />



    <EditText
        android:id="@+id/editTextpass"
        android:layout_width="250dp"
        android:layout_height="45dp"
        android:layout_marginTop="70dp"
        android:background="@drawable/btnbg"
        android:elevation="7dp"
        android:textSize="15dp"
        android:gravity="center_vertical"
        android:paddingStart="30dp"
        android:textColor="@color/black"
        android:fontFamily="@font/robotoregular"
        android:textColorHint="@color/grey"
        android:inputType="textPassword"
        android:paddingEnd="45dp"
        android:hint="Password to save ..."
        app:layout_constraintEnd_toEndOf="@+id/view8"
        app:layout_constraintStart_toStartOf="@+id/view8"
        app:layout_constraintTop_toTopOf="@+id/view8" />

    <EditText
        android:id="@+id/logid"
        android:layout_width="250dp"
        android:layout_height="45dp"
        android:layout_marginTop="70dp"
        android:background="@drawable/btnbg"
        android:elevation="7dp"
        android:fontFamily="@font/robotoregular"
        android:gravity="center_vertical"
        android:paddingStart="30dp"
        android:hint="Login id... "
        android:textColor="@color/black"
        android:textColorHint="@color/grey"
        android:inputType="text|textNoSuggestions|textVisiblePassword"
        android:maxLines="1"
        android:textSize="15dp"
        app:layout_constraintEnd_toEndOf="@+id/viewnewbg"
        app:layout_constraintStart_toStartOf="@+id/viewnewbg"
        app:layout_constraintTop_toTopOf="@+id/view7" />

    <TextView
        android:id="@+id/cancel"
        android:layout_width="100dp"
        android:layout_height="40dp"
        android:background="@drawable/usebtn"
        android:gravity="center"
        android:text="Cancel"
        android:textColor="@color/white"
        android:textSize="15dp"
        android:layout_marginTop="10dp"
        app:layout_constraintEnd_toEndOf="@+id/view8"
        app:layout_constraintStart_toStartOf="@+id/view8"
        app:layout_constraintTop_toBottomOf="@id/save"
        tools:layout_editor_absoluteY="548dp" />

    <TextView
        android:id="@+id/save"
        android:layout_width="120dp"
        android:layout_height="45dp"
        android:background="@drawable/usebtn"
        android:layout_marginBottom="100dp"
        android:gravity="center"
        android:text="Save"
        android:textColor="@color/white"
        android:textSize="15dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/view8"
        app:layout_constraintStart_toStartOf="@+id/view8" />

    <View
        android:id="@+id/showP"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginEnd="15dp"
        android:background="@drawable/ic_baseline_remove_red_eye_24"
        android:elevation="7dp"
        app:layout_constraintBottom_toBottomOf="@+id/editTextpass"
        app:layout_constraintEnd_toEndOf="@+id/editTextpass"
        app:layout_constraintTop_toTopOf="@+id/editTextpass" />

</androidx.constraintlayout.widget.ConstraintLayout>