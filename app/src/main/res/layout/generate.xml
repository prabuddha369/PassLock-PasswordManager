<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bgm">

    <View
        android:id="@+id/backtomain"
        android:layout_width="50dp"
        android:layout_height="40dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/ic_baseline_arrow_back_ios_24"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textviewnew"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:fontFamily="@font/manropebold"
        android:text="Create\nUnique\nPassword"
        android:textColor="@color/white"
        android:textSize="30dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/backtomain"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/viewnewbg"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginHorizontal="30dp"
        android:layout_marginTop="180dp"
        android:layout_marginBottom="30dp"
        android:background="@drawable/mandombg"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textviewnew"
        app:layout_constraintVertical_bias="0.0" />

    <View
        android:id="@+id/view4"
        android:layout_width="match_parent"
        android:layout_height="250dp"
        android:layout_marginHorizontal="30dp"
        android:background="@drawable/mandombg"
        android:layout_marginBottom="149dp"
        app:layout_constraintBottom_toBottomOf="@+id/viewnewbg"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <SeekBar
        android:id="@+id/capsseek"
        android:layout_width="100dp"
        android:layout_height="30dp"
        android:layout_marginTop="50dp"
        android:layout_marginStart="25dp"
        android:progress="30"
        android:progressTint="@color/green"
        android:thumbTint="@color/green"
        app:layout_constraintStart_toStartOf="@+id/viewnewbg"
        app:layout_constraintTop_toBottomOf="@+id/textviewnew" />

    <SeekBar
        android:id="@+id/digitseek"
        android:layout_width="100dp"
        android:layout_height="30dp"
        android:layout_marginTop="50dp"
        android:progress="30"
        android:progressTint="@color/pink"
        android:thumbTint="@color/pink"
        app:layout_constraintStart_toEndOf="@+id/capsseek"
        app:layout_constraintTop_toBottomOf="@+id/textviewnew" />

    <SeekBar
        android:id="@+id/specialseek"
        android:layout_width="100dp"
        android:layout_height="30dp"
        android:layout_marginTop="50dp"
        android:progress="20"
        android:progressTint="@color/lightblue"
        android:thumbTint="@color/lightblue"
        app:layout_constraintHorizontal_bias="0.032"
        app:layout_constraintStart_toEndOf="@+id/digitseek"
        app:layout_constraintTop_toBottomOf="@+id/textviewnew" />

    <TextView
        android:id="@+id/captxt"
        android:layout_width="50dp"
        android:layout_height="20dp"
        android:layout_marginStart="45dp"
        android:layout_marginTop="22dp"
        android:fontFamily="@font/manropebold"
        android:gravity="center_horizontal"
        android:text="3"
        android:textColor="@color/green"
        app:layout_constraintStart_toStartOf="@+id/viewnewbg"
        app:layout_constraintTop_toTopOf="@+id/capsseek" />

    <TextView
        android:id="@+id/spetxt"
        android:layout_width="50dp"
        android:layout_height="20dp"
        android:layout_marginStart="250dp"
        android:layout_marginTop="22dp"
        android:fontFamily="@font/manropebold"
        android:gravity="center_horizontal"
        android:text="2"
        android:textColor="@color/lightblue"
        app:layout_constraintStart_toStartOf="@+id/viewnewbg"
        app:layout_constraintTop_toTopOf="@+id/capsseek" />

    <TextView
        android:id="@+id/digtxt"
        android:layout_width="50dp"
        android:layout_height="20dp"
        android:layout_marginStart="145dp"
        android:layout_marginTop="22dp"
        android:fontFamily="@font/manropebold"
        android:gravity="center_horizontal"
        android:text="3"
        android:textColor="@color/pink"
        app:layout_constraintStart_toStartOf="@+id/viewnewbg"
        app:layout_constraintTop_toTopOf="@+id/capsseek" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="300dp"
        android:layout_height="30dp"
        android:layout_marginTop="18dp"
        android:fontFamily="@font/robotoregular"
        android:text="\t\t\t\tCaps\t\t\t\t\t\t\t\t\tDigits\t\t\t\t\t\t\t\tSpecials"
        android:textColor="@color/black"
        android:textSize="15dp"
        app:layout_constraintEnd_toEndOf="@+id/viewnewbg"
        app:layout_constraintStart_toStartOf="@+id/viewnewbg"
        app:layout_constraintTop_toBottomOf="@+id/digitseek" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="250dp"
        android:layout_height="250dp"
        android:src="@drawable/generatecircle"
        app:layout_constraintEnd_toEndOf="@+id/viewnewbg"
        app:layout_constraintStart_toStartOf="@+id/viewnewbg"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />



    <TextView
        android:id="@+id/gen_pass"
        android:layout_width="300dp"
        android:layout_height="45dp"
        android:layout_marginTop="50dp"
        android:background="@drawable/btnbg"
        android:textColorHint="@color/grey"
        android:hint="Click Generate ..."
        android:paddingStart="15dp"
        android:textColor="@color/black"
        android:paddingEnd="40dp"
        android:textSize="16dp"
        android:gravity="center_vertical"
        android:elevation="5dp"
        app:layout_constraintEnd_toEndOf="@+id/view4"
        app:layout_constraintStart_toStartOf="@+id/view4"
        app:layout_constraintTop_toTopOf="@+id/view4" />

    <View
        android:id="@+id/regen"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="5dp"
        android:background="@drawable/regenerate"
        android:elevation="6dp"
        app:layout_constraintEnd_toEndOf="@+id/gen_pass"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/gen_pass"
        app:layout_constraintTop_toTopOf="@+id/gen_pass" />

    <TextView
        android:id="@+id/usebtn"
        android:layout_width="90dp"
        android:layout_height="45dp"
        android:layout_marginTop="25dp"
        android:background="@drawable/usebtn"
        android:gravity="center"
        android:text="Use"
        android:textColor="@color/white"
        android:textSize="15dp"
        app:layout_constraintEnd_toEndOf="@+id/view4"
        app:layout_constraintStart_toStartOf="@+id/view4"
        app:layout_constraintTop_toBottomOf="@+id/gen_pass" />

    <TextView
        android:id="@+id/generatebtn"
        android:layout_width="120dp"
        android:layout_height="45dp"
        android:layout_marginTop="25dp"
        android:background="@drawable/usebtn"
        android:gravity="center"
        android:text="Generate"
        android:textColor="@color/white"
        android:textSize="15dp"
        app:layout_constraintEnd_toEndOf="@+id/view4"
        app:layout_constraintHorizontal_bias="0.501"
        app:layout_constraintStart_toStartOf="@+id/view4"
        app:layout_constraintTop_toBottomOf="@+id/gen_pass" />

    <TextView
        android:id="@+id/total"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_marginTop="70dp"
        android:fontFamily="@font/manropebold"
        android:gravity="center"
        android:text="8"
        android:textColor="@color/black"
        android:textSize="40dp"
        app:layout_constraintEnd_toEndOf="@+id/imageView3"
        app:layout_constraintStart_toStartOf="@+id/imageView3"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

</androidx.constraintlayout.widget.ConstraintLayout>