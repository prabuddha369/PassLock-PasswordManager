<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/googlelid"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bgm"
    tools:context=".Google">

    <View
        android:id="@+id/backgoogle"
        android:layout_width="50dp"
        android:layout_height="40dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/ic_baseline_arrow_back_ios_24"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/newpass"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:layout_marginTop="48dp"
        android:layout_marginEnd="10dp"
        android:background="@drawable/btnnew"
        android:elevation="10dp"
        android:gravity="center"
        android:text="  New"
        android:textColor="@color/black"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/dom"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:fontFamily="@font/manropebold"
        android:gravity="center"
        android:text="Domain"
        android:textColor="@color/black"
        android:textSize="22dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="214dp" />

    <View
        android:id="@+id/view3"
        android:layout_width="359dp"
        android:layout_height="339dp"
        android:background="@drawable/mandombg"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/dom" />

    <TextView
        android:id="@+id/hint"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:layout_marginTop="150dp"
        android:gravity="center"
        android:textColor="@color/grey"
        android:text="Click + New to add a password"
        app:layout_constraintEnd_toEndOf="@+id/view3"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="@+id/view3"
        app:layout_constraintTop_toTopOf="@+id/view3" />

    <TextView
        android:id="@+id/social"
        android:layout_width="85dp"
        android:layout_height="40dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="30dp"
        android:background="@drawable/btnbg"
        android:elevation="5dp"
        android:fontFamily="@font/robotoregular"
        android:gravity="center"
        android:text="Social"
        android:textColor="@color/black"
        android:textSize="20dp"
        app:layout_constraintStart_toStartOf="@+id/view3"
        app:layout_constraintTop_toBottomOf="@+id/dom" />

    <TextView
        android:id="@+id/google"
        android:layout_width="85dp"
        android:layout_height="40dp"
        android:layout_marginStart="95dp"
        android:layout_marginTop="30dp"
        android:background="@drawable/btnbg"
        android:elevation="10dp"
        android:fontFamily="@font/robotoregular"
        android:gravity="center"
        android:text="Google"
        android:textColor="@color/black"
        android:textSize="20dp"
        app:layout_constraintStart_toStartOf="@+id/view3"
        app:layout_constraintTop_toBottomOf="@+id/dom" />

    <TextView
        android:id="@+id/more"
        android:layout_width="85dp"
        android:layout_height="40dp"
        android:layout_marginStart="250dp"
        android:layout_marginTop="30dp"
        android:background="@drawable/btnbg"
        android:elevation="20dp"
        android:fontFamily="@font/robotoregular"
        android:gravity="center"
        android:text="More"
        android:textColor="@color/black"
        android:textSize="20dp"
        app:layout_constraintStart_toStartOf="@+id/view3"
        app:layout_constraintTop_toBottomOf="@+id/dom" />
    <TextView
        android:id="@+id/ott"
        android:layout_width="85dp"
        android:layout_height="40dp"
        android:layout_marginStart="173dp"
        android:layout_marginTop="30dp"
        android:background="@drawable/btnbg"
        android:elevation="15dp"
        android:fontFamily="@font/robotoregular"
        android:gravity="center"
        android:text="OTT"
        android:textColor="@color/black"
        android:textSize="20dp"
        app:layout_constraintStart_toStartOf="@+id/view3"
        app:layout_constraintTop_toBottomOf="@+id/dom" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/RVparent"
        android:layout_width="300dp"
        android:layout_height="500dp"
        android:layout_marginTop="30dp"
        app:layout_constraintEnd_toEndOf="@+id/view3"
        app:layout_constraintStart_toStartOf="@+id/view3"
        app:layout_constraintTop_toBottomOf="@+id/google" />

</androidx.constraintlayout.widget.ConstraintLayout>